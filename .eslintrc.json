{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["@typescript-eslint"],
  "rules": {
    "import/prefer-default-export": "off",
    "object-curly-newline": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "no-unused-vars": "warn",
    "no-shadow": "off",
    "no-console": "off"
  }
}
